{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters } from 'vuex';\nimport Topbar from './topbar.vue';\nimport { showSweetAlert } from '../../Store/utils/sweetalert';\nimport axios from 'axios';\nimport { FlowerSpinner } from 'epic-spinners';\nexport default {\n  components: {\n    Topbar,\n    FlowerSpinner\n  },\n  computed: {\n    ...mapGetters(['isAuth', 'getUserData']),\n    showLogin() {\n      return !this.isAuth;\n    }\n  },\n  data() {\n    return {\n      loading: false,\n      form: {\n        name: '',\n        email: '',\n        company: '',\n        message: ''\n      }\n    };\n  },\n  methods: {\n    clickSubmitForm() {\n      this.loading = true;\n    },\n    async sendEmail() {\n      try {\n        const {\n          name,\n          email,\n          message\n        } = this.form;\n        await axios.post('http://localhost:3000/api/send-email', {\n          name,\n          email,\n          text: message\n        });\n        showSweetAlert('success', 'ส่งอีเมลสำเร็จ', false, 1500);\n        this.loading = true;\n        setTimeout(() => {\n          this.$router.push('/');\n        }, 3000);\n        this.loading = false;\n      } catch (err) {\n        showSweetAlert('error', 'มีข้อผิดพลาด', false, 1500);\n        console.log(err);\n      }\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","Topbar","showSweetAlert","axios","FlowerSpinner","components","computed","showLogin","isAuth","data","loading","form","name","email","company","message","methods","clickSubmitForm","sendEmail","post","text","setTimeout","$router","push","err","console","log"],"sources":["C:\\Users\\Ball\\OneDrive\\Documents\\Web dev\\own-website\\src\\components\\FrontPage\\contact.vue"],"sourcesContent":["<template>\r\n    <Topbar/>\r\n    <div v-if=\"!loading\"\r\n        class=\"border rounded-xl shadow-xl mt-20 pt-8 p-6 w-2/3 sm:w-1/2 lg:w-1/3 m-auto\">\r\n        <h3 class=\"text-base sm:text-xl font-semibold\"> Any questio