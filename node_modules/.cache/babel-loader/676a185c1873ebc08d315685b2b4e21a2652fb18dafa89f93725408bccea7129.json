{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapGetters } from 'vuex';\nimport Topbar from './topbar.vue';\nimport nodemailer from 'nodemailer';\nimport { showSweetAlert } from '../utils/sweetalert';\nimport { routerKey } from 'vue-router';\nexport default {\n  components: {\n    Topbar\n  },\n  computed: {\n    ...mapGetters(['isAuth', 'getUserData']),\n    showLogin() {\n      return !this.isAuth;\n    }\n  },\n  data() {\n    return {\n      type: false,\n      values: {\n        name: '',\n        email: '',\n        company: '',\n        message: ''\n      }\n    };\n  },\n  methods: {\n    sendEmail() {\n      let transporter = nodemailer.createTransport({\n        host: \"smtp.google.com\",\n        port: 587,\n        secure: false,\n        // upgrade later with STARTTLS\n        auth: {\n          user: process.env.MAIL_USER,\n          pass: process.env.MAIL_PASSWORD\n        }\n      });\n      let mailBody = {\n        from: process.env.MAIL_USER,\n        to: this.$store.getters.user.email,\n        subject: 'Thank you for your email.',\n        text: `Hi ${values.name},\\n\n                    Thank you for your email. I will get back you go asap.\\n\n                    Best regards,\\n\n                    Ball Thatthana`\n      };\n      transporter.sendMail(mailBody, (err, info) => {\n        if (err) {\n          showSweetAlert('error', err, false, 1500);\n          console.log(err);\n          console.log(mailBody);\n        } else {\n          showSweetAlert('success', info, false, 1500);\n          router.push('/');\n        }\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapGetters","Topbar","nodemailer","showSweetAlert","routerKey","components","computed","showLogin","isAuth","data","type","values","name","email","company","message","methods","sendEmail","transporter","createTransport","host","port","secure","auth","user","process","env","MAIL_USER","pass","MAIL_PASSWORD","mailBody","from","to","$store","getters","subject","text","sendMail","err","info","console","log","router","push"],"sources":["C:\\Users\\Ball\\OneDrive\\Documents\\Web dev\\own-website\\src\\components\\FrontPage\\contact.vue"],"sourcesContent":["<template>\r\n    <Topbar/>\r\n    <div class=\"border rounded-xl shadow-xl mt-20 pt-8 p-6 w-2/3 sm:w-1/2 lg:w-1/3 m-auto\">\r\n\r\n        <h3 class=\"text-base sm:text-xl font-semibold\"> Any questions, please fill following form to contact me.\r\n        </h3>\r\n\r\n        <div class=\"signin_container p_top mt-4 py-4 px-2 m-auto\">\r\n            <form @submit.prevent=\"sendEmail\">\r\n\r\n                <div class=\"form-group\">\r\n                        <input\r\n                            name=\"name\"\r\n                            type=\"text\"\r\n                            id=\"name\"\r\n                            class=\"form-control mt-2 bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\r\n                            placeholder=\"Enter your name\"\r\n                            v-model=\"values.name\"\r\n                        />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                        <input\r\n                            name=\"company\"\r\n                            type=\"text\"\r\n                            id=\"company\"\r\n                            class=\"form-control mt-2 bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\r\n                            placeholder=\"Enter your company\"\r\n                            v-model=\"values.company\"\r\n                        />\r\n                </div>\r\n    \r\n                <div class=\"form-group\">\r\n                        <input\r\n                            name=\"email\" \r\n                            type=\"text\"\r\n                            id=\"email\"\r\n                            class=\"form-control mt-2 bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\r\n                            placeholder=\"Enter your email\"\r\n                            v-model=\"values.email\"\r\n                        />\r\n                </div>\r\n\r\n                <div class=\"form-group\">\r\n                        <textarea\r\n                            name=\"textarea\" \r\n                            type=\"text\"\r\n                            id=\"textarea\"\r\n                            class=\"form-control mt-2 bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\r\n                            placeholder=\"Enter your email\"\r\n                            v-model=\"values.message\">\r\n                         </textarea>\r\n                </div>\r\n\r\n                <button\r\n                    type=\"submit\"\r\n                    class=\"btn mb-3 btn-block mt-2 text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm w-full sm:w-auto px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800\"\r\n                > Send message\r\n                </button>\r\n                <hr/>\r\n            </form>\r\n        </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapGetters } from 'vuex';\r\nimport Topbar from './topbar.vue'\r\nimport nodemailer from 'nodemailer'\r\nimport { showSweetAlert } from '../utils/sweetalert'\r\nimport { routerKey } from 'vue-router';\r\n\r\nexport default {\r\n    components: {\r\n        Topbar\r\n    },\r\n    computed:{\r\n        ...mapGetters(['isAuth', 'getUserData']),\r\n        showLogin(){\r\n            return !this.isAuth;\r\n        }\r\n    },\r\n    data(){\r\n        return {\r\n            type:false,\r\n            values: {\r\n                name:'',\r\n                email:'',\r\n                company:'',\r\n                message: ''\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n        sendEmail(){\r\n            let transporter = nodemailer.createTransport({\r\n            host: \"smtp.google.com\",\r\n            port: 587,\r\n            secure: false, // upgrade later with STARTTLS\r\n            auth: {\r\n                user: process.env.MAIL_USER,\r\n                pass: process.env.MAIL_PASSWORD\r\n                },\r\n            });\r\n\r\n            let mailBody = {\r\n                from: process.env.MAIL_USER,\r\n                to: this.$store.getters.user.email,\r\n                subject: 'Thank you for your email.',\r\n                text: `Hi ${values.name},\\n\r\n                    Thank you for your email. I will get back you go asap.\\n\r\n                    Best regards,\\n\r\n                    Ball Thatthana`\r\n            };\r\n\r\n            transporter.sendMail(mailBody,(err,info)=>{\r\n                if(err){\r\n                    showSweetAlert('error', err, false, 1500)\r\n                    console.log(err)\r\n                    console.log(mailBody)\r\n                } else {\r\n                    showSweetAlert('success',info , false, 1500)\r\n                    router.push('/')\r\n                }\r\n            })\r\n        }\r\n    }\r\n}\r\n\r\n</script>"],"map