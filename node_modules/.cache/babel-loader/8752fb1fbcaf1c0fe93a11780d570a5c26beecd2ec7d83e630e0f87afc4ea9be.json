{"ast":null,"code":"import { mapActions, mapGetters } from 'vuex';\nimport Topbar from './topbar.vue';\nimport Banner from './banner.vue';\nimport Portfolio from './portfolio.vue';\nimport { showSweetAlert } from '../../Store/utils/sweetalert.js';\nexport default {\n  components: {\n    Topbar,\n    Banner,\n    Portfolio\n  },\n  computed: {\n    ...mapGetters(['isAuth']),\n    showLogin() {\n      return !this.isAuth;\n    }\n  },\n  data() {\n    return {\n      type: false,\n      signInAndSignUpComplete: false,\n      values: {\n        email: '',\n        password: ''\n      }\n    };\n  },\n  watch: {\n    signInAndSignUpComplete(value) {\n      if (value && !this.type) {\n        showSweetAlert('success', 'เข้าสู่ระบบสำเร็จ', false, 1500);\n      } else if (value && this.type) {\n        showSweetAlert('success', 'ลงทะเบียนสำเร็จ', false, 1500);\n      } else if (!value) {\n        showSweetAlert('success', 'ออกจากระบบสำเร็จ', false, 1500);\n      }\n    }\n  },\n  methods: {\n    ...mapActions(['signin', 'signup', 'signOut']),\n    async onSubmit() {\n      try {\n        if (!this.type) {\n          // sign in\n          await this.$store.dispatch('signin', this.values);\n        } else {\n          //sign up\n          await this.$store.dispatch('signup', this.values);\n        }\n      } catch (err) {\n        if (err.response && err.response.data && err.response.data.error.message === 'INVALID_PASSWORD') {\n          showSweetAlert('error', 'รหัสไม่ถูกต้อง', false, 1500);\n        } else {\n          showSweetAlert('error', err, false, 1500);\n        }\n      }\n    },\n    onLogOut() {\n      this.$store.dispatch('signOut');\n      //this.signInAndSignUpComplete = false;\n    }\n  }\n};","map":{"version":3,"names":["mapActions","mapGetters","Topbar","Banner","Portfolio","showSweetAlert","components","computed","showLogin","isAuth","data","type","signInAndSignUpComplete","values","email","password","watch","value","methods","onSubmit","$store","dispatc