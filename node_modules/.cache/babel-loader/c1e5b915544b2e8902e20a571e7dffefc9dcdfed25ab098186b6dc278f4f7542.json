{"ast":null,"code":"import { mapActions, mapGetters } from 'vuex';\nimport Topbar from './topbar.vue';\nimport Banner from './banner.vue';\nimport Portfolio from './portfolio.vue';\nimport { showSweetAlert } from '../../Store/utils/sweetalert';\nexport default {\n  components: {\n    Topbar,\n    Banner,\n    Portfolio\n  },\n  computed: {\n    ...mapGetters(['isAuth']),\n    showLogin() {\n      return !this.isAuth;\n    }\n  },\n  data() {\n    return {\n      type: false,\n      signInAndSignUpComplete: false,\n      values: {\n        email: '',\n        password: ''\n      }\n    };\n  },\n  watch: {\n    signInAndSignUpComplete(value) {\n      if (!value && !this.type) {\n        showSweetAlert('success', 'เข้าสู่ระบบสำเร็จ', false, 1500);\n      } else if (value && this.type) {\n        showSweetAlert('success', 'ลงทะเบียนสำเร็จ', false, 1500);\n      }\n    }\n  },\n  methods: {\n    ...mapActions(['signin', 'signup', 'signOut']),\n    onSubmit() {\n      if (!this.type) {\n        // sign in\n        this.$store.dispatch('signin', this.values);\n        this.signInComplete = true;\n        console.log(this.type);\n      } else {\n        //sign up\n        this.$store.dispatch('signup', this.values);\n        this.signInComplete = true;\n        console.log(this.type);\n      }\n    },\n    onLogOut() {\n      this.$store.dispatch('signOut');\n    }\n  }\n};","map":{"version":3,"names":["mapActions","mapGetters","Topbar","Banner","Portfolio","showSweetAlert","components","computed","showLogin","isAuth","data","type","signInAndSignUpComplete","values","email","password","watch","value","methods","onSubmit","$store","dispatch","signInComplete","console","log","onLogOut"],"sources":["C:\\Users\\Ball\\OneDrive\\Documents\\Web dev\\own-website\\src\\components\\FrontPage\\home.vue"],"sourcesContent":["<template>\r\n    <div>\r\n        <div v-if=\"showLogin\" class=\"border rounded-xl shadow-xl mt-10 pt-8 p-6 w-2/3 md:w-1/3 m-auto\">\r\n            <h3 class=\"text-base sm:text-xl font-semibold\"> Please \r\n                <span v-text=\"!type ? 'sign in':'sign up'\"></span>\r\n                to see my personal information.\r\n            </h3>\r\n\r\n            <div class=\"signin_container p_top mt-4 py-4 px-2 m-auto\">\r\n                <form @submit.prevent=\"onSubmit\">\r\n\r\n\r\n                    <div class=\"form-group\">\r\n                            <input\r\n                                name=\"email\" \r\n                                type=\"text\"\r\n                                id=\"email\"\r\n                                class=\"form-control mt-2 bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\r\n                                placeholder=\"Enter your email\"\r\n                                v-model=\"values.email\"\r\n                            />\r\n                    </div>\r\n\r\n\r\n                    <div class=\"form-group\">\r\n                            <input\r\n                                name=\"password\"\r\n                                type=\"password\"\r\n                                id=\"password\"\r\n                                class=\"form-control mt-2 bg-gray-50 border border-gray-300 text-gray-900 text-sm rounded-lg focus:ring-blue-500 focus:border-blue-500 block w-full p-2.5 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-blue-500 dark:focus:border-blue-500\"\r\n                                placeholder=\"Enter your password\"\r\n                                v-model=\"values.password\"\r\n                            />\r\n                    </div>\r\n\r\n                    <button\r\n                        type=\"submit\"\r\n                        class=\"btn mb-3 btn-block mt-2 text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg text-sm w-full sm:w-auto px-5 py-2.5 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800\"\r\n                        v-text=\"!type ? 'Sign in':'Sign up'\"\r\n                    >\r\n                    </button>\r\n\r\n                    <hr/>\r\n                    <div class=\"form_swap\"> Click to \r\n                        <span @click=\"type = !type\">\r\n                            <span v-if=\"type\">\r\n                                <b>Sign in</b>\r\n                            </span>\r\n                            <span v-else>\r\n                                <b>Sign up</b>\r\n                            </span>\r\n                        </span>\r\n                    </div>\r\n                </form>\r\n            </div>\r\n        </div>\r\n        <div v-else>\r\n            <div class=\"text-center py-2\">\r\n                <span>You are signed In </span>\r\n                    <button \r\n                        @click=\"onLogOut\"\r\n                        class=\"mt-2 text-white bg-blue-700 hover:bg-blue-800 focus:ring-4 focus:outline-none focus:ring-blue-300 font-medium rounded-lg sm:w-auto px-2 text-center dark:bg-blue-600 dark:hover:bg-blue-700 dark:focus:ring-blue-800\"\r\n                        >Logout\r\n                    </button>\r\n            </div>\r\n            <Topbar/>\r\n            <Banner/>\r\n            <Portfolio/>\r\n            </div>\r\n    </div>\r\n</template>\r\n\r\n<script>\r\nimport { mapActions, mapGetters } from 'vuex';\r\nimport Topbar from './topbar.vue'\r\nimport Banner from './banner.vue'\r\nimport Portfolio from './portfolio.vue'\r\nimport { showSweetAlert } from '../../Store/utils/sweetalert';\r\n\r\nexport default {\r\n    components:{\r\n        Topbar,\r\n        Banner,\r\n        Portfolio,\r\n    },\r\n    computed:{\r\n        ...mapGetters(['isAuth']),\r\n        showLogin(){\r\n            return !this.isAuth;\r\n        }\r\n    },\r\n    data(){\r\n        return {\r\n            type:false,\r\n            signInAndSignUpComplete: false,\r\n            values: {\r\n                email:'',\r\n                password:'',\r\n            }\r\n        }\r\n    },\r\n    watch: {\r\n        signInAndSignUpComplete(value){\r\n            if(!value && !this.type){\r\n                showSweetAlert('success', 'เข้าสู่ระบบสำเร็จ', false, 1500);\r\n            } else if(value && this.type){\r\n                showSweetAlert('success', 'ลงทะเบียนสำเร็จ', false, 1500);\r\n            }\r\n        }\r\n    },\r\n    methods:{\r\n        ...mapActions(['signin', 'signup', 'signOut']),\r\n        \r\n        onSubmit(){\r\n            if(!this.type){\r\n                // sign in\r\n                this.$store.dispatch('signin', this.values);\r\n                this.signInComplete = true;\r\n                console.log(this.type)\r\n              \r\n            } else {\r\n                //sign up\r\n                this.$store.dispatch('signup', this.values);\r\n                this.signInComplete = true;\r\n                console.log(this.type)\r\n            }\r\n        },\r\n        onLogOut(){\r\n            this.$store.dispatch('signOut')\r\n        }\r\n    }\r\n}\r\n</script>"],"mappings":"AAyEA,SAASA,UAAU,EAAEC,UAAS,QAAS,MAAM;AAC7C,OAAOC,MAAK,MAAO,cAAa;AAChC,OAAOC,MAAK,MAAO,cAAa;AAChC,OAAOC,SAAQ,MAAO,iBAAgB;AACtC,SAASC,cAAa,QAAS,8BAA8B;AAE7D,eAAe;EACXC,UAAU,EAAC;IACPJ,MAAM;IACNC,MAAM;IACNC;EACJ,CAAC;EACDG,QAAQ,EAAC;IACL,GAAGN,UAAU,CAAC,CAAC,QAAQ,CAAC,CAAC;IACzBO,SAASA,CAAA,EAAE;MACP,OAAO,CAAC,IAAI,CAACC,MAAM;IACvB;EACJ,CAAC;EACDC,IAAIA,CAAA,EAAE;IACF,OAAO;MACHC,IAAI,EAAC,KAAK;MACVC,uBAAuB,EAAE,KAAK;MAC9BC,MAAM,EAAE;QACJC,KAAK,EAAC,EAAE;QACRC,QAAQ,EAAC;MACb;IACJ;EACJ,CAAC;EACDC,KAAK,EAAE;IACHJ,uBAAuBA,CAACK,KAAK,EAAC;MAC1B,IAAG,CAACA,KAAI,IAAK,CAAC,IAAI,CAACN,IAAI,EAAC;QACpBN,cAAc,CAAC,SAAS,EAAE,mBAAmB,EAAE,KAAK,EA